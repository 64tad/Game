import java.io.IOException;
import java.net.URL;

public class Classwork
{
	public static void main(String args[])
	{
		try 
		{
			URL url = new URL("http://www.smcisd.net/");
			String content = (String) url.getContent();
		} 
		catch (IOException e) 
		{
			e.printStackTrace();
		}
		
		System.out.println(content);
	}
}